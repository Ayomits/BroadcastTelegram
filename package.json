{
  "name": "replicator-bot",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc --build && tsc-alias",
    "start:tg": "node dist/telegram/main.js",
    "start:ds": "node dist/discord/main.js",
    "start:sr": "node dist/server/main.js",
    "dev:tg": "nodemon --config nodemon.telegram.json",
    "dev:ds": "nodemon --config nodemon.discord.json",
    "dev:sr": "nodemon --config nodemon.server.json",
    "dev": "concurrently --kill-others \"pnpm run dev:tg\" \"pnpm run dev:ds\" \"pnpm run dev:sr\""
  },
  "imports": {
    "#/*": ".//dist/*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.5",
    "@types/node": "^24.9.1",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/hydrate": "^1.6.0",
    "@grammyjs/menu": "^1.3.1",
    "@grammyjs/parse-mode": "^2.2.0",
    "@qz/telegram-entities-parser": "npm:@jsr/qz__telegram-entities-parser@^1.0.1",
    "@typegoose/typegoose": "^12.20.0",
    "amqplib": "^0.10.9",
    "axios": "^1.12.2",
    "ayologger": "^0.0.11",
    "concurrently": "^9.2.1",
    "discord.js": "^14.24.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "grammy": "^1.38.3",
    "ioredis": "^5.8.2",
    "mongoose": "^8.19.3"
  }
}

    "@grammyjs/commands": "^1.2.0",
    "@grammyjs/hydrate": "^1.6.0",
    "@grammyjs/menu": "^1.3.1",
    "@grammyjs/parse-mode": "^2.2.0",
    "@typegoose/typegoose": "^12.20.0",
    "axios": "^1.12.2",
    "ayologger": "^0.0.11",
    "concurrently": "^9.2.1",
